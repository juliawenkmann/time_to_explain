{
  "name": "subgraphx_tg",
  "adapter": "subgraphx_tg",
  "args": {
    "explanation_level": "event",
    "results_dir": "resources/results",
    "debug_mode": false,
    "threshold_num": 20,
    "save_results": true,
    "mcts_saved_dir": "resources/results/tgnnexplainer_subgraphx/mcts_saved_dir",
    "load_results": false,
    "rollout": 500,
    "min_atoms": 100,
    "c_puct": 5.0,
    "use_navigator": true,
    "navigator_type": "mlp",
    "navigator_params": {
      "train_epochs": 100,
      "lr": 0.0001,
      "batch_size": 16,
      "reg_coefs": [
        0.5,
        0.1
      ],
      "explainer_ckpt_dir": "resources/explainer/tgnnexplainer"
    },
    "cache": true,
    "alias": "subgraphx_tg"
  }
}
